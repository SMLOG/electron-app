  "version": "0.2.0",
  "configurations": [
    {
      "name": "babel",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "program": "${file}",
      "type": "node",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/babel-node",
      "sourceMaps": true,
      "env": {
        "BABEL_ENV": "debug"
      }
    }
  ]
}
