    "promise-polyfill": "^8.1.3",
    "sequelize": "^6.3.5",
    "socket.io": "^2.3.0",
    "storejs": "^1.0.25",
    "vue": "^2.6.11",
    "vue-calendar-component": "^2.8.2",
    "vue-echarts": "^5.0.0-beta.0",
    "vue-electron": "^1.0.6",
