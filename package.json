{
  "name": "suspension",
  "version": "0.0.1",
  "author": "Lee <1614369925@qq.com>",
  "description": "An electron-vue project",
  "license": "",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node build/build.js && electron-builder",
    "build:dir": "node build/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node build/build.js",
    "build:web": "cross-env BUILD_TARGET=web node build/build.js",
    "devs": "nodemon --exitcrash -w src/server --exec \"babel-node bin/www\"",
    "pm2": "pm2 start \"npm run devs\" && pm2 log",
    "pma": "pm2 start \"npm run devs\" && pm2 start \"npm run devc\" && pm2 log",
    "devc": "node build/dev-runner.js",
    "dev": "concurrently \"set NODE_ENV=development&&nodemon -w src/server --exec \\\"babel-node bin/www\\\"\" \"node build/dev-runner.js\"",
    "start": "git pull && node build/dev-runner.js",
    "pack": "npm run pack:server && npm run pack:web",
    "pack:server": "cross-env NODE_ENV=production webpack --progress --colors --config build/webpack.server.config.js",
    "pack:web": "cross-env NODE_ENV=production webpack --progress --colors --config build/webpack.web.config.js",
    "postinstall": ""
  },
  "build": {
    "productName": "Test",
    "appId": "com.example.yourapp",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "dependencies": {
    "axios": "^0.19.2",
    "cluster": "^0.7.7",
    "cron": "^1.8.2",
    "debug": "^4.1.1",
    "echarts": "^4.9.0",
    "electron-fetch": "^1.4.0",
    "electron-pdf-window": "^1.0.12",
    "fetch-jsonp": "^1.1.3",
    "hammerjs": "^2.0.8",
    "jquery": "^3.5.1",
